{
	"name": "ActivityLog_ImportFromStageToArchive",
	"properties": {
		"activities": [
			{
				"name": "Copy From Staging To Archive",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlMISource",
						"sqlReaderQuery": "Select * from activityLogs with (NoLock)\n    WHERE  Cast(datetime as date) = Cast(getdate()-14 as date)",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ActivityLogs_StagingTableSource",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Archive_Destination",
						"type": "DatasetReference",
						"parameters": {
							"DestinationTable": "ActivityLogs",
							"DestinationSchema": "BusinessData"
						}
					}
				]
			}
		],
		"folder": {
			"name": "DataRetention/MISC"
		},
		"annotations": []
	}
}